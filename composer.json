{
    "name": "codebar-ag/laravel-flatfox",
    "description": "Flatfox integration with Laravel",
    "keywords": [
        "flatfox",
        "laravel",
        "codebar-ag",
        "laravel-flatfox"
    ],
    "homepage": "https://github.com/codebar-ag/laravel-zammad",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Steiger",
            "email": "sebastian.fix@codebar.ch",
            "homepage": "https://www.codebar.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^10.0",
        "sammyjo20/saloon": "^2.6",
        "sammyjo20/saloon-cache-plugin": "^2.1",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.4.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "2.x-dev",
        "pestphp/pest-plugin-laravel": "2.x-dev",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ray": "^1.9"

    },
    "autoload": {
        "psr-4": {
            "CodebarAg\\Flatfox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodebarAg\\Flatfox\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodebarAg\\Flatfox\\FlatfoxServiceProvider"
            ],
            "aliases": {
                "Flatfox": "CodebarAg\\Flatfox\\Facades\\Flatfox"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
